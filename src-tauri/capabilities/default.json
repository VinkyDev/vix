{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "hidden-webview"],
  "remote": {
    "urls": ["*://*/*"]
  },
  "permissions": [
    "core:default",
    "opener:default",
    "os:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "shell:default",
    "core:event:default",
    "core:window:allow-set-size",
    "log:default"
  ]
}
